{
  "expo": {
    "name": "Shop Scanner",
    "slug": "ShopScannerUniversal",
    "version": "3.6.0",
    "scheme": "shopscanner",
    "orientation": "portrait",
    "icon": "./assets/app_icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash_screen_final.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.shopscanner.wave",
      "googleServicesFile":"./GoogleService-Info.plist"
     
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/ic_launcher_foreground.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.shopscanner.wave",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "@react-native-firebase/app",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Poppins-Black.ttf",
            "./assets/fonts/Assistant-VariableFont_wght.ttf",
            "./assets/fonts/Poppins-SemiBold.ttf"
          ]
        }
      ],
      "expo-localization",
      [
        "expo-tracking-transparency",
        {
          "userTrackingPermission": "This identifier will be used to deliver personalized deals for you."
        }
      ],
      [
      "expo-notifications",
      {
        "icon": "./assets/notification_icon.png",
        "color": "#ffffff"
       
      }
    ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2ff3cdc7-f57f-4cad-8c93-1d796083c770"
      }
    },
    "entryPoint": ".vscode\\exponentIndex.js"
  },
  "name": "shopscanneruniversal"
}
