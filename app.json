{
  "expo": {
    "name": "Shop Scanner",
    "slug": "ShopScannerUniversal",
    "version": "6.0.0",
    "scheme": "shopscanner",
    "orientation": "portrait",
    "icon": "./assets/app_icon.png",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.shopscanner.wave",
      "googleServicesFile":"./GoogleService-Info.plist"
     
     
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/ic_launcher_foreground.png",
        "backgroundColor": "#ffffff"
      },
     
      "package": "com.shopscanner.wave",
      "googleServicesFile": "./google-services.json",
      "config": {
        "googleSignIn": {
          "certificateHash": "34:DD:8B:C7:B2:ED:E4:52:6C:66:4A:60:AA:FF:27:08:3C:25:8F:8E"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "@react-native-firebase/app",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Poppins-Black.ttf",
            "./assets/fonts/Assistant-VariableFont_wght.ttf",
            "./assets/fonts/Poppins-SemiBold.ttf"
          ]
        }
      ],
      "expo-localization",
      [
        "expo-tracking-transparency",
        {
          "userTrackingPermission": "This identifier will be used to deliver personalized deals for you."
        }
      ],
      [
      "expo-notifications",
      {
        "icon": "./assets/notification_icon.png",
        "color": "#ffffff",
        "sound": "default", 
        "vibration": true 
       
      }
    ],
    "@react-native-google-signin/google-signin"
    
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2ff3cdc7-f57f-4cad-8c93-1d796083c770"
      }
    },
    "entryPoint": ".vscode\\exponentIndex.js"
  },
  "name": "shopscanneruniversal"
}
