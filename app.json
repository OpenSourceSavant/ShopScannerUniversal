{
  "expo": {
    "name": "ShopScannerUniversal",
    "slug": "ShopScannerUniversal",
    "version": "1.2.0",
    "scheme": "shopscanneruniversal",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.shopscanner.wave",
      "googleServicesFile": "./google-services.json" 
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Poppins-Black.ttf",
            "./assets/fonts/Assistant-VariableFont_wght.ttf",
            "./assets/fonts/Poppins-SemiBold.ttf"
            
          ]
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2ff3cdc7-f57f-4cad-8c93-1d796083c770"
      }
    }
  }
}
